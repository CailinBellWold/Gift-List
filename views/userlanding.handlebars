        <div class='container-fluid'>
            <section id='btn-add-gift' class='add-gift-btn'>
                <div class='d-flex flex-row-reverse col-12'>
                    <button class='btn btn-warning'>+ Add Gift</button>
                </div>
            </section>
            
            <section class='gift-list'>
                <div class='col-12'>
                    <div class='card'>
                        <h3 class='card-header'>My Gift List</h3>
                        <table class='table table-hover'>
                            <thead>
                                <tr>
                                    <th scope='col'>Recipient</th>
                                    <th scope='col'>Description</th>
                                    <th scope='col'>Gift Complete</th>
                                    <th scope='col'></th>
                                </tr>
                            </thead>
                    {{#if gifts.length}}
                        {{#each gifts as |gift|}}
                        <tbody>
                            <tr><a href='/gift/{{gift.id}}'></a>
                                <td scope='row'>{{gift.recipientName}}</th>
                                <td id='description'>{{gift.description}}</td>
                                <td id='purchased'>{{#if gift.purchased}}Yes{{else}}No{{/if}}</td>
                                <td class='edit-btns'>
                                    <button class='btn btn-warning edit-gift-btn' data-id='{{gift.id}}'>Update</button>
                                    <button id="deleteBtn" class='btn btn-danger delete-gift-btn' data-id='{{gift.id}}'>Delete</button>
                                    
                                </td>
                            </tr>
                        </tbody>
                        {{/each}}
                    {{/if}}
                    </div>
                </div>
            </section>
        </div>

<script src='./js/userlanding.js'></script>